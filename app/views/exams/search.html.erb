<% if signed_in? %>
	<h1>Search DICOM Images</h1>
	<%= render 'exams/index' %>
	<%= form_tag("/search", method: "get") do %>
	<%= label_tag(:tag, "Tag:") %>
	<%= text_field_tag(:tag) %>
	<%= label_tag(:val, "Value:") %>
	<%= text_field_tag(:val) %>
	<%= submit_tag("Search") %>
	<% end %>
<% else %>
	<%= render 'static_pages/noLogin' %>
<% end %>